{
  ".source.mathematica": {
    "editor": {
      "commentStart": "(*",
      "commentEnd": "*)"
    },
    "autocomplete": {
      "symbols": {
        "function": {
          "selector": ".entity.name.function",
          "typePriority": 3
        },
        "variable": {
          "selector": ".variable.other, .variable.parameter, .variable.language",
          "typePriority": 4
        },
        "constant": {
          "selector": ".constant.other, .constant.language",
          "typePriority": 2
        },
        "builtin": {
          "selector": ".keyword.function.builtin",
          "typePriority": 5
        }
      }
    }
  }
}
